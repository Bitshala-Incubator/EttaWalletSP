  "version": "0.0.1",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "react-native start",
    "start:reset": "react-native start --reset-cache",
    "android:dev": "react-native run-android --no-packager",
    "ios:dev": "react-native run-ios --no-packager",
    "android:reset": "run-p android:dev start:reset",
    "ios:reset": "run-p ios:dev start:reset",
